{
  "expo": {
    "name": "WLED Glow",
    "slug": "wled-glow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.glowpermanentlights.dev.ios.app",
      "buildNumber": "24061812",
      "supportsTablet": true,
      "infoPlist": {
        "NSBonjourServices": [
          "my_service._tcp.",
          "my_other_service._tcp."
        ],
        "NSLocalNetworkUsageDescription": "Allow WLED Glow to find wled compatible devices on your network"
      }
    },
    "android": {
      "package": "com.glowpermanentlights.dev.android.app",
      "versionCode": 24061812,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "CHANGE_WIFI_MULTICAST_STATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0eea1c4e-38f7-4edb-93e1-ada2530b890b"
      }
    }
  }
}
